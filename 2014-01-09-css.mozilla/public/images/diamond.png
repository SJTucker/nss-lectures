<html xmlns="http://www.w3.org/1999/xhtml"> 
    <head> 
        <link rel="stylesheet" href="./auth-styles.css"/> 
        <title>Captive Portal</title> 
    </head> 
    <body> 
    <div class="splash-container" style="background-color:#33ccff;"> 
        <div class="splash-banner" style="background-color:#ffffff;"> 
            <div style="word-wrap:break-word" id="splash-banner-text" class="splash-banner-text">EMMA Inc. Guest WiFi</div> 
        </div> 
        <div class="splash-body-container"> 
            <div style="word-wrap:break-word" id="splash-terms" class="splash-block splash-terms"> 
                Welcome to the EMMA Inc sponsored free Guest WiFi! 
            </div> 
            <div class="splash-block splash-policy"> 
                <div style="word-wrap:break-word" id="splash-policy" class="splash-policy-container"> 
                    This is a Guest WiFi for specific Use. You acknowledge that you are to use it as a guest only, and that your use of this service is honorable and ethical only. No Right to Privacy is granted. 
                </div> 
                <div> 
	                <input type="radio" name="agree" id="agree-yes" value="true"><label for="agree-yes">I agree</label> 
	                <input type="radio" name="agree" id="agree-no" value="false"><label for="agree-no">I do not agree</label> 
                </div> 
            </div> 
            <div class="splash-block splash-form"> 
                <form method="POST" action="swarm.cgi" onsubmit='if(!document.getElementById("agree-yes").checked) {alert("Please accept the terms of use by clicking I agree in the box at left."); return false;} if(!document.getElementById("user").value || !document.getElementById("password").value) {alert("Invalid username/password."); return false;}'> 
                    <input type="hidden" name="orig_url" value="687474703a2f2f6d6f7a6f72672e63646e2e6d6f7a696c6c612e6e65742f6d656469612f696d672f686f6d652f70616e656c2f6c696768746265616d2f6c6f676f2d6c696768746265616d2d68697265732e706e67" /> 
                    <input type="hidden" name="opcode" value="cp_auth" /> 
                    User Name: <br/> <input type="text" name="user" id="user"/> 
                    <br/> 
                    Password: <br/> <input type="password" name="password" id="password"/> 
                    <br/> 
                    <input type="submit" value="Login"/> 
                </form> 
                  
            </div> 
        </div> 
    </div> 
    </body> 
    <script> 
        (function () { 
            var decode_string = ''; 
            var decode_map = { 
                'banner' : 'splash-banner-text', 
                'terms' : 'splash-terms', 
                'policy' : 'splash-policy' 
            } 
            function unicodeToText(srcText) { 
                var text = ''; 
                if(srcText) { 
                    var arr = srcText.split(';'); 
                    for ( var i = 0; i < arr.length; i++ ){ 
                        if (arr[i] !== '') { 
                            text += String.fromCharCode(parseInt(arr[i], 16)); 
                        } 
                    } 
                } 
                return text; 
            } 
            function $(id) { 
                return document.getElementById(id); 
            } 
            if(decode_string) { 
                var decode_texts = decode_string.split('/'); 
                var id, element; 
                for (var i = 0; i < decode_texts.length; i++) { 
                    id = decode_map[decode_texts[i]]; 
                    if (id) { 
                        element = $(id); 
                        if (element) { 
                            element.innerHTML = unicodeToText(element.innerHTML); 
                        } 
                    } 
                } 
            } 
        })() 
    </script> 
</html>
